{
    "rules": {
        "indent": [
            2,
            4,
            {"SwitchCase": 1}
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "eqeqeq": 2,
        "no-unused-vars": 1
    },
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "jquery": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "arrowFunctions": true,
        "blockBindings": true,
        "experimentalObjectRestSpread": true
    },
    "plugins": [
        "react"
    ]
}
